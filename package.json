  },
  "homepage": "https://github.com/ProcStack/pxlNav#readme",
  "peerDependencies": {
    "three": "^0.171.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "esbuild": "^0.24.0",
    "rollup": "^4.28.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
