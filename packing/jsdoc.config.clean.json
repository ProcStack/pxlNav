{
  "source": {
    "include": ["./src/pxlNav"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./jsdoc",
    "recurse": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts":{
      "default_theme": "dark",
      "search": true,
      "menu": [
        {
          "title": "pxlNav Example : The Outlet",
          "link": "https://procstack.github.io/Outlet.htm",
          "target": "_blank",
          "class": "menu-item",
          "id": "pxlNav_example_theOutlet"
        }
      ]
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}